
@{
    ViewBag.Title = "ExamTeamHomePage";
    Layout = "~/Views/Shared/_layout.cshtml";
}

@section ExamTeamHomePage{
    <style>
        div.IssueSel {
            border-style: solid;
            border-color: #b3d7ff;
        }
    </style>
    <h2>Technical Issue Status</h2>
    <table style="width:100%;" border="1">
        <tr><th>Issue ID</th><th>Building</th><th>Lab No</th><th>PC No</th><th>Problem Type</th><th>Date</th><th>Recieved Time</th><th>Priority</th><th>Status</th></tr>
        @foreach (var std in ViewData["TechIssues"] as IList<ExamTeamManagementSystem.Models.TechnicalIssue>)
        {
            <tr><td>@std.TechnicalIssueID</td><td>@std.BuildingNo</td><td>@std.LabNo</td><td>@std.PCNo</td><td>@std.ProblemType</td><td>@std.Date_Tech</td><td>@std.Time_Tech</td><td>@std.Priority</td><td>@std.Status</td></tr>
        }
    </table>
    <h2>Logistics Issue Status</h2>
    <table style="width:100%;" border="1">
        <tr><th>Issue ID</th><th>Building</th><th>Lab No</th><th>PC No</th><th>Problem Type</th><th>Date</th><th>Recieved Time</th><th>Priority</th><th>Status</th></tr>
        @foreach (var std in ViewData["LogIssues"] as IList<ExamTeamManagementSystem.Models.LogisticsIssue>)
        {
            <tr><td>@std.LogisticsIssueID</td><td>@std.BuildingNo</td><td>@std.LabNo</td><td>@std.PCNo</td><td>@std.ProblemType</td><td>@std.Date_Log</td><td>@std.Time_Log</td><td>@std.Priority</td><td>@std.Status</td></tr>
        }
    </table>

    <!-- <table style="width:20%;">
        <tr><td><input id="Button1" type="button" value="button" /></td><td><input id="Button1" type="button" value="button" /></td><td><input id="Button1" type="button" value="button" /></td></tr>
        <tr><td><input id="Button1" type="button" value="button" /></td><td><input id="Button1" type="button" value="button" /></td><td><input id="Button1" type="button" value="button" /></td></tr>
        <tr><td><input id="Button1" type="button" value="button" /></td><td><input id="Button1" type="button" value="button" /></td><td><input id="Button1" type="button" value="button" /></td></tr>
    </table> -->
    <table style="width:100%;">
        <tr>
            <td>
                &nbsp;
                <div class="IssueSel form-group">

                    <table style="width:20%;">
                       <tr><td><input id="Button3" type="button" onclick="location.href='/ExamTeam/IssuePage'" value="Go To Issue Page" /></td></tr>
                    </table>
                </div>
            </td>
        </tr>
    </table>
}