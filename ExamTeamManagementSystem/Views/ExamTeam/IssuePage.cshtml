
@{
    ViewBag.Title = "IssuePage";
    Layout = "~/Views/Shared/_layout.cshtml";
}

<script language="javascript" type="text/javascript">

    window.history.forward();
</script>

@if (Session["UserName"] != null)
{

    @section IssueSelection{
        <style>


            div.IssueSel {
                border-style: solid;
                border-color: #b3d7ff;
            }
        </style>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <table style="width:100%;">
                <tr>
                    <td>
                        &nbsp;
                        <div class="IssueSel">
                            <h2>Select Issue</h2>
                            <table style="width:20%;">
                                <tr><td><input style="height:50px;width:200px" id="Button1" type="button" onclick="location.href='/ExamTeam/BuildingSelectionPageTech'" value="Technical Support" /></td></tr>
                                <tr><td><input style="height:50px;width:200px" id="Button2" type="button" onclick="location.href='/ExamTeam/BuildingSelectionPage'" value="Logistics Support" /></td></tr>
                            </table>
                        </div>
                    </td>
                </tr>
            </table>
            <table style="width:100%;">
                <tr>
                    <td>
                        &nbsp;
                        <div class="IssueSel form-group">

                            <table style="width:20%;">
                                <tr><td><input id="Button1" type="button" onclick="location.href='/ExamTeam/ExamTeamHomePage'" value="Back" /></td></tr>
                                <tr><td><input id="Button3" type="button" onclick="location.href='@Url.Action("LogOutAction", "ExamTeam")'" value="LogOut" /></td></tr>
                            </table>
                        </div>
                    </td>
                </tr>
            </table>

        }
    }
}
else
{
    < h2 > User Logged Out</ h2 >
}