
@{
    ViewBag.Title = "ITHomePage";
    Layout = "~/Views/Shared/_layout.cshtml";
}
<script language="javascript" type="text/javascript">

    window.history.forward();
</script>
<style>


    div.IssueSel {
        border-style: solid;
        border-color: #b3d7ff;
    }
</style>
@if (Session["UserName"] != null)
{


    @section IThomeselection{

        <table style="width:100%;">
            <tr>
                <td>
                    &nbsp;
                    <div class="IssueSel">
                        <h2>Select an option below</h2>
                        <table style="width:20%;">
                            <tr><td><input style="height:50px;width:200px" id="Button1" type="button" onclick="location.href='/ExamTeam/TechnicalSupportIssuePage'" value="Create New Issue" /></td></tr>
                            <tr><td><input style="height:50px;width:200px" id="Button2" type="button" onclick="location.href='/ITUnitTeam/ITTechnicalIssuePage'" value="Issues Inquiry" /></td></tr>
                        </table>
                    </div>
                </td>
            </tr>
            <tr><td><input id="Button3" type="button" onclick="location.href='@Url.Action("LogOutAction", "ExamTeam")'" value="LogOut" /></td></tr>
        </table>
    }
}
else
{
    < h2 > User Logged Out</ h2 >
}
